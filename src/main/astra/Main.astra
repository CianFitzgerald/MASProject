agent Main {
    module Console C;
    module System S;
	module EIS ei;
    module Debug debug;

	rule +!main(list args) {
		ei.launch("vw","dependency/vacuumworld-1.2.0.jar");
		ei.init([generation("no")]);
		ei.join();
		ei.startEnv();

		list E = ei.freeEntities();
		forall(string ent : E) {
			C.println("E="+E);
		}
		ei.link("Decco");
		ei.move("forward", 5);
	}
}
